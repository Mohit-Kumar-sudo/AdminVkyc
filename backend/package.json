  "scripts": {
    "clean": "rimraf dist",
    "dev:build": "npm run clean && cross-env NODE_OPTIONS='--max-old-space-size=8192' webpack --mode development && cross-env NODE_ENV=development node dist/index.bundle.js",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' webpack --mode production",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS='--max-old-space-size=8192' node dist/index.bundle.js",
    "start": "node dist/index.bundle.js",
    "inspect": "cross-env NODE_ENV=development node --inspect-brk dist/index.bundle.js",
    "prettier": "prettier --single-quote --print-width 80 --trailing-comma all --write 'src/**/*.js'",
    "preinstall": "npm install -g webpack"
  },
